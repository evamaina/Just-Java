<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Just Java</string>

    <!-- Header text for list of toppings [CHAR LIMIT=20]-->
    <string name="toppings">Toppings</string>


    <!-- Hint text for the name of the user  [CHAR LIMIT=20]-->
    <string name="name">Name</string>


    <!--  Text for the whipped cream [CHAR LIMIT=20]-->
    <string name="whipped_cream">Whipped cream</string>

    <!--  Text for the chocolate cream [CHAR LIMIT=20]-->
    <string name="chocolate_cream">Chocolate</string>

    <!--  Text for the Quantity [CHAR LIMIT=20]-->
    <string name="quantity_text">Quantity</string>

    <!--  Text for the decrement operator [CHAR LIMIT=20]-->
    <string name="decrement">-</string>

    <!--  Text for the increment operator [CHAR LIMIT=20]-->
    <string name="increment">+</string>

    <!--  Text for the initial value [CHAR LIMIT=20]-->
    <string name="two">2</string>

    <!--  Text for the order button [CHAR LIMIT=20]-->
    <string name="order">Order</string>


    <!-- Name for the order summary.It will be shown in the format of "Name:Eva"
    where Eva is the users name [CHAR LIMIT=NONE]-->
    <string name="order_summary_name">Name: <xliff:g id="name" example="Eva">%s</xliff:g> </string>

    <!-- Quantity of coffee cups for the order summary.It will be shown in the format of "Quantity:2"
    where 2 is the number of cups ordered [CHAR LIMIT=NONE]-->
    <string name="order_summary_quantity">Quantity: <xliff:g id="quantity" example="2">%d</xliff:g> </string>

    <!-- whipped cream topping topping for the order summary.It will be shown in the format of "Add whipped cream? true"
     or "add whipped cream? false" [CHAR LIMIT=NONE]-->
    <string name="order_summary_whipped_cream">Add whipped cream: <xliff:g id="addWhippedCream" example="true">%s</xliff:g> </string>

    <!-- chocolate topping for the order summary.It will be shown in the format of "Add chocolate? true"
     or "add chocolate? false" [CHAR LIMIT=NONE]-->
    <string name="order_summary_chocolate">Add chocolate: <xliff:g id="addChocolate" example="true">%s</xliff:g> </string>


    <!--
      Total price for the order summary. It will be shown in the format of
      "Total: $20" where $20 is the price. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_price">Total: <xliff:g id="price" example="$20">%s</xliff:g></string>

    <!-- Thank you message for the order summary [CHAR LIMIT=NONE]-->
    <string name="thank_you">Thank You!</string>

    <!--
      Subject line for the order summary email. It will be in the format of
      "Just Java order for Eva" where Eva is the user's name. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_email_subject">Just Java order for Eva <xliff:g id="name" example="Eva">%s</xliff:g></string>



</resources>
